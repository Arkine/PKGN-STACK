{
	"extends": "eslint:recommended", 
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"amd": true
	},
    "rules": {
        "indent": ["error", "tab"],
		"react/jsx-uses-vars": [2],
		"no-console": 0
    }
}